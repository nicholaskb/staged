###  http://qudt.org/schema/qudt/unit
qudt:unit rdf:type owl:AnnotationProperty ;
          rdfs:subPropertyOf gist:hasUnitOfMeasure .


###  https://ontologies.semanticarts.com/gist/governs
gist:governs rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/hasMagnitude
gist:hasMagnitude rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/hasPart
gist:hasPart rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/hasSubTask
gist:hasSubTask rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/hasUnitOfMeasure
gist:hasUnitOfMeasure rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/isBasedOn
gist:isBasedOn rdf:type owl:AnnotationProperty .


###  https://ontologies.semanticarts.com/gist/produces
gist:produces rdf:type owl:AnnotationProperty .


###  https://w3id.org/cmc-stagegate#definesProcess
ex:definesProcess rdfs:comment "Plan defines processes as subtasks" ;
                  rdf:type owl:AnnotationProperty ;
                  rdfs:subPropertyOf gist:hasSubTask .


###  https://w3id.org/cmc-stagegate#evaluatedBy
ex:evaluatedBy rdfs:comment "Method governs what/how to measure" ;
               rdf:type owl:AnnotationProperty ;
               rdfs:subPropertyOf gist:governs .


###  https://w3id.org/cmc-stagegate#hasEvidence
ex:hasEvidence rdfs:comment "Specification grounded in analytical results" ;
               rdf:type owl:AnnotationProperty ;
               rdfs:subPropertyOf gist:isBasedOn .


###  https://w3id.org/cmc-stagegate#hasGate
ex:hasGate rdfs:comment "Stage contains gate review as part" ;
           rdf:type owl:AnnotationProperty ;
           rdfs:subPropertyOf gist:hasPart .


###  https://w3id.org/cmc-stagegate#hasPlan
ex:hasPlan rdfs:comment "Stage contains plan as part of its structure" ;
           rdf:type owl:AnnotationProperty ;
           rdfs:subPropertyOf gist:hasPart .


###  https://w3id.org/cmc-stagegate#hasSpecification
ex:hasSpecification rdfs:comment "Design-time specification link" ;
                    rdf:type owl:AnnotationProperty ;
                    rdfs:subPropertyOf gist:hasPart .


###  https://w3id.org/cmc-stagegate#hasUnitOperation
ex:hasUnitOperation rdfs:comment "Process contains unit operations as subtasks" ;
                    rdf:type owl:AnnotationProperty ;
                    rdfs:subPropertyOf gist:hasSubTask .


###  https://w3id.org/cmc-stagegate#measuredOnLot
ex:measuredOnLot rdfs:comment "Lot has magnitude/measurement result" ;
                 rdf:type owl:AnnotationProperty ;
                 rdfs:subPropertyOf gist:hasMagnitude .


###  https://w3id.org/cmc-stagegate#producesMaterial
ex:producesMaterial rdfs:comment "Process produces material output" ;
                    rdf:type owl:AnnotationProperty ;
                    rdfs:subPropertyOf gist:produces .


#################################################################
#    Object Properties
#################################################################

###  https://ontologies.semanticarts.com/gist/hasParticipant
gist:hasParticipant rdf:type owl:ObjectProperty ;
                    owl:inverseOf ex:assessedAtGate .


###  https://ontologies.semanticarts.com/gist/isAspectOf
gist:isAspectOf rdf:type owl:ObjectProperty ;
                owl:inverseOf ex:hasMeasurementFor .


###  https://ontologies.semanticarts.com/gist/isCategorizedBy
gist:isCategorizedBy rdf:type owl:ObjectProperty .


###  https://w3id.org/cmc-stagegate#assessedAtGate
ex:assessedAtGate rdf:type owl:ObjectProperty ;
                  rdfs:comment "Result assessed at gate event" .


###  https://w3id.org/cmc-stagegate#consumesMaterial
ex:consumesMaterial rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf gist:hasParticipant ;
                    rdfs:comment "Process consumes material as participant" .


###  https://w3id.org/cmc-stagegate#hasLot
ex:hasLot rdf:type owl:ObjectProperty ;
          owl:inverseOf ex:lotOf ;
          rdfs:domain ex:Material ;
          rdfs:range ex:Lot ;
          rdfs:comment "Material has associated lots" .


###  https://w3id.org/cmc-stagegate#hasMeasurementFor
ex:hasMeasurementFor rdf:type owl:ObjectProperty ;
                     rdfs:comment "Links measurement to its quality attribute (inverse of isAspectOf)" .


###  https://w3id.org/cmc-stagegate#lotOf
ex:lotOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf gist:isCategorizedBy ;
         rdfs:domain ex:Lot ;
         rdfs:range ex:Material ;
         rdfs:comment "Links lot to its material type" .


